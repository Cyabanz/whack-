# Hyperbeam API Configuration
HYPERBEAM_API_KEY=your_hyperbeam_api_key_here

# Security Configuration
CSRF_SECRET=your_random_32_char_csrf_secret_here

# Optional: Custom session timeouts (in milliseconds)
# SESSION_TIMEOUT=600000  # 10 minutes (default)
# INACTIVITY_TIMEOUT=30000  # 30 seconds (default)

# Optional: Rate limiting configuration
# RATE_LIMIT_REQUESTS=10  # Max requests per minute per IP (default)
# RATE_LIMIT_WINDOW=60000  # Rate limit window in milliseconds (default)

# Next.js Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000